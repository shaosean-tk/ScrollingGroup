  <ItemName>UpdateScroller</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub UpdateScroller()</SourceLine>
   <SourceLine>self.Scroller.PageStep = (self.Viewer.Height)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if (self.ListCount = 0) then</SourceLine>
   <SourceLine>self.Scroller.Maximum = 0</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>if (self.FormattedHeight &gt; self.Viewer.Height) then</SourceLine>
   <SourceLine>self.Scroller.Maximum = (self.FormattedHeight - self.Viewer.Height)</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>self.Scroller.Maximum = 0</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
