  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub Invalidate(eraseBackground As Boolean = True)</SourceLine>
   <SourceLine>DIM top As Integer = (0 - self.Scroller.Value)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if (UBound(self.mContainers) &gt; -1) then</SourceLine>
   <SourceLine>for i as Integer = 0 to UBound(self.mContainers)</SourceLine>
   <SourceLine>self.mContainers(i).Top = top</SourceLine>
   <SourceLine>top = top + self.mContainers(i).Height</SourceLine>
   <SourceLine>next</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>self.UpdateScroller</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Super.Invalidate eraseBackground</SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
